<table>
	<thead>
		<tr>
			<th>Student</th>
			<th>Grade</th>
			<th>Marks</th>
		</tr>

	</thead>
	<tbody>
	<% @marksheets.each do |ms| %>
	<tr>
		<td><%=ms.student_name%></td>
		<td><%=ms.grade%></td>
		<td><%=ms.marks%></td>
	</tr>
	<% end %>
		</tbody>
</table>

<%= link_to "Back to grades" , root_path %> || <%= link_to "Edit marksheets" , edit_grade_marksheet_path(@grade) %>
